	nCRetIva        += nRetIVA
	nTCRetIVA       += nRetIVA
	nCRetIBB        += nRetIBB
	nTCRetIBB       += nRetIBB
	nCExento        += _nExentas //  * nSigno)
	nTCExento       += _nExentas //  * nSigno)
