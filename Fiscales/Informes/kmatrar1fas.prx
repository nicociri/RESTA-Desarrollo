	nFRetIBB        += nRetIBB
	nTFRetIBB       += nRetIBB
	nFExento        += _nExentas  // * nSigno)
	nTFExento       += _nExentas  // * nSigno)
	nFImpTotal      += (nValBrut   * nSigno)
	nTFImpTotal     += (nValBrut   * nSigno)
EndIf

