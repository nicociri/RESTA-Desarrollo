		MsUnLock()

		cTexto += "Foi incluํdo o relacionamento " + aSX9[nI][1] + "/" + aSX9[nI][3] + CRLF

		oProcess:IncRegua2( "Atualizando Arquivos (SX9)..." )

	EndIf

Next nI

