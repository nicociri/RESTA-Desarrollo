User Function NomeSF2()
Local cNome	:= Space(40)

If AllTrim(SF2->F2_ESPECIE) $ 'NCP'
	cNome	:= Posicione('SA2', 1, xFilial('SA2') + SF2->( F2_CLIENTE + F2_LOJA ), 'A2_NOME' )
Else
	cNome	:= Posicione('SA1', 1, xFilial('SA1') + SF2->( F2_CLIENTE + F2_LOJA ), 'A1_NOME' )
EndIf

Return( cNome ) 