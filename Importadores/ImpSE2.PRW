Static Function GrabaSE2(aCabec,aImport)

Local nX 		:= 0
Local nCpo      := 0
Local cCod      := ""
Local nPosCod   := 0
Local nPosLoj   := 0
Local nCantErr  := 0
Local lsalir    := .F.
Local cDirLog   := "\log\"
Local cSubdir   := "anticipos\"
Local nPosPre   := 0
Local nPosNum   := 0
Local nPosPar   := 0
Local nPosTip   := 0
Local nPosFor   := 0
Local nPosLoj   := 0
Local aNoCpos   := {"E2_FILIAL"} // campos que seran excluidos en la rutina 
      
Private lMsErroAuto 	:= .F.
Private aVector       	:= {}

ProcRegua(Len(aImport))

nPosPre := aScan( aCabec,{|x| allTrim(x) == "E2_PREFIXO" })
nPosNum := aScan( aCabec,{|x| allTrim(x) == "E2_NUM" })
nPosPar := aScan( aCabec,{|x| allTrim(x) == "E2_PARCELA" })
nPosTip := aScan( aCabec,{|x| allTrim(x) == "E2_TIPO" })
nPosFor := aScan( aCabec,{|x| allTrim(x) == "E2_FORNECE" })
nPosLoj := aScan( aCabec,{|x| allTrim(x) == "E2_LOJA" })

If nPosPre+nPosNum+nPosPar+nPosTip+nPosFor+nPosLoj == 0
	MsgAlert('No se localizaron los campos del ํndice de SE2','Atenci๓n')
	Return
Endif

For nX:=1 To Len(aImport)
	IncProc("Importaci๓n: Insertando registro "+AllTrim( Str(nX) )+" de "+AllTrim( Str(Len(aImport)) ))
	
	aVector  := {}
	
	DbSelectArea("SE2") //E2_FILIAL+E2_PREFIXO+E2_NUM+E2_PARCELA+E2_TIPO+E2_FORNECE+E2_LOJA
	DbSetOrder(1)
	
	If !DbSeek( xFilial("SE2")+aImport[nX][nPosPre]+aImport[nX][nPosNum]+aImport[nX][nPosPar]+;
	                           aImport[nX][nPosTip]+aImport[nX][nPosFor]+aImport[nX][nPosLoj])
		
		For nCpo := 1 To Len(aCabec)
			If !Empty(aCabec[nCpo]) .And. aScan( aNoCpos,{|x| x == Alltrim(aCabec[nCpo]) }) == 0
		
				Aadd(aVector,{Alltrim(aCabec[nCpo]),valorCpo(aCabec[nCpo],aImport[nX][nCpo]),Nil})
				
			Endif
		Next nCpo
		
		MSExecAuto({|x,y,z| fina050(x,y,z)},aVector,Nil,3)   
						
		If lMsErroAuto
			ProcLogAtu("ERRO",'Atenci๓n',"Error en la importaci๓n de anticipos")
			If !ExistDir(cDirLog)		
				MakeDir(cDirLog)
			EndIf
			If !ExistDir(cDirLog+cSubdir)		
				MakeDir(cDirLog+cSubdir)
			EndIf
			MOSTRAERRO(cDirLog+cSubdir)
			lMsErroAuto := .F.
			DisarmTransaction()
			nCantErr += 1
		EndIf	
	EndIf	
	
	If lsalir
		Exit
	Endif
Next

If nCantErr > 0
	MsgAlert("Fin del Proceso. Registros con errores: "+str(nCantErr),'Atenci๓n')
Else
	MsgAlert('Proceso finalizado con ้xito','Atenci๓n')
Endif

//End Transaction

Return


