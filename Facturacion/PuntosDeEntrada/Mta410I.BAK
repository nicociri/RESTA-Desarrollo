  				aadd(aRet,{SC6->C6_PRODUTO,SC6->C6_VALDESC,SC6->C6_DESCONT,SC6->C6_PRCVEN,SC6->C6_VALOR,SC6->C6_PRUNIT,SC6->C6_ENTREG,SC6->C6_DESCRI})
    	SC6->(DbSkip())
   		EndDo
  
RestArea(_aAreaSC6)

return (aRet)      

