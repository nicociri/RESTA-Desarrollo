Static Function MaCanAltOrc()

Local lRetorno := .T.

If ( !SCJ->CJ_STATUS $ "A#D#E#F" )
	lRetorno := .F.
Else
	If Select("TMP1")<>0
		lRetorno := Empty(SCK->CK_NUMOP)
	EndIf
EndIf

If !lRetorno 
	lRetAux := lRetorno
EndIf

Return(lRetorno)
